    "eslint-plugin-testing-library": "^4.6.0",
    "gh-pages": "^3.2.3",
    "history": "^5.0.0",
    "jest": "^27.0.1",
    "jest-cli": "^27.0.1",
    "jest-styled-components": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "styled-components": "^5.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.1",
