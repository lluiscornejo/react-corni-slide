{
  "name": "react-corni-slider",
  "version": "1.0.4",
  "description": "Native horizontal scrolling carousel",
  "main": "./dist/index.js",
  "types": "./lib/index.js",
  "homepage": "https://lluiscornejo.github.io/react-corni-slide",
  "scripts": {
    "clean": "rimraf dist lib",
    "start": "cross-env NODE_ENV=development webpack --watch",
    "build": "npm run clean && cross-env NODE_ENV=production webpack && babel ./src --out-dir ./lib",
    "test-watch": "jest --watch",
    "test": "jest",
    "test-coverage": "jest --coverage --passWithNoTests",
    "bundle-visualizer": "cross-env SHOW_GRAPH=true webpack --profile --json > stats.json",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o public",
    "deploy-storybook": "gh-pages -d public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lluiscornejo/react-corni-slide"
  },
  "author": "Luis Cornejo",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/builder-webpack5": "^6.3.0",
    "@storybook/manager-webpack5": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.12.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
